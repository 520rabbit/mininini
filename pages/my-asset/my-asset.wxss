/* pages/my-asset/my-asset.wxss */
.my-record {
  background-image: linear-gradient(to right, #FFB146, #FF8954);
  width: 690rpx;
  margin: auto;
  border-radius: 20rpx;
  overflow: hidden;
}

.my-record::after {
  position: absolute;
  top: 350rpx;
  left: 0rpx;
  display: block;
  content: '';
  width: 750rpx;
  height: 20rpx;
  background: #f5f5f5;
  z-index: 99;
}

.my-record > .deal {
  color: #fff;
  text-align: right;
  line-height: 60rpx;
  font-size: 30rpx;
  margin-top: 20rpx;
  padding-right:30rpx;
  box-sizing: border-box;
}

.my-record > .have-count {
  color: #fff;
  text-align: center;
  font-size: 78rpx;
}

.my-record > .count-title {
  color: #FFC8B2;
  text-align: center;
  font-size: 30rpx;
}

.money-take {
  display: flex;
  justify-content: space-between;
  background: rgba(255,255,255,.1);
  padding: 10rpx 0;
  box-sizing: border-box;
}

.money-take .all-coin {
  width: 50%;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  position: relative;
}

.money-take .all-coin::after {
  position: absolute;
  top: 20rpx;
  right: 0;
  display: block;
  content: '';
  width: 2rpx;
  height: 40rpx;
  background: #fff;
  opacity: .8;
}

.money-take .use-coin {
  width: 50%;
  text-align: center;
  color: #fff;
  font-size: 28rpx;
  opacity: .8;
}

.money {
  margin-top: 60rpx;
  padding-left:  30rpx;
  box-sizing: border-box;
}

.money > .title {
  font-size: 32rpx;
  line-height: 70rpx;
}

.money .money-item {
  display: inline-block;
  padding: 20rpx 0;
  box-sizing: border-box;
  width: 210rpx;
  background: #FD9769;
  border-radius: 10rpx;
  color: #fff;
  height: 130rpx;
  font-size: 30rpx;
  margin-right: 30rpx;
  margin-bottom: 30rpx;
}


/* 底部导航 */
.footer {
  position: fixed;
  bottom: 0;
  left: 0;
  background: #fff;
  width: 100vw;
  border-top: 2rpx solid #F6F6F6; 
  display: flex;
  justify-content: left;
  padding: 15rpx 30rpx;
  box-sizing: border-box;
}

.footer > .back {
  width: 250rpx;
  background: #fff;
  border: 2rpx solid #DEDEDE;
  line-height: 68rpx;
  text-align: center;
  font-size: 30rpx;
  color: #959595;
  border-radius: 10rpx;
  margin-right: 20rpx;
  box-sizing: border-box;
}

.save {
  flex: 1;
  background: linear-gradient(to right, #FFC36F, #FF8853);
  line-height: 68rpx;
  text-align: center;
  border-radius: 10rpx;
  color: #fff;
  font-size: 30rpx;
}

.save > .btn {
  width: 30rpx;
  height: 30rpx;
  margin-right: 30rpx;
}


/* 弹层 */

.mask {
  background: rgba(000, 000, 000, .6);
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  z-index: 999;
}

.success {
  width: 590rpx;
  margin: 100rpx auto;
  background: #fff;
  padding-bottom: 50rpx;
  box-sizing: border-box;
  border-radius: 20rpx;
}

.success .top-popover {
  width: 100%;
  border-radius: 20rpx;
  background: url(data:image/png;base64,) no-repeat;
  height: 420rpx;
  background-size: contain;
  overflow: hidden;

}

.mask .state{
  display: block;
  width: 205rpx;
  height: 205rpx;
  margin: 48rpx auto 0 auto;
}

.success > .update-success {
  font-size: 42rpx;
  color: #FF8F5D;
  text-align: center;
  line-height: 75rpx;
}

.success > .toast {
  width: 460rpx;
  margin: auto;
  line-height: 48rpx;
  text-align: center;
  color: #959595;
  font-size: 30rpx;
}

.success > .kown {
  width: 390rpx;
  margin: 60rpx auto;
  line-height: 100rpx;
  border-radius: 10rpx;
  background: #FF8853;
  text-align: center;
  color: #fff;
}